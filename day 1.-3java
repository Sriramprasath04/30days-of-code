class Solution{
    static int searchInSorted(int arr[], int N, int K)
    {
        
        // Your code here
        int f=0;
        int l=N-1;
        int res = -1;
        while(f<=l){
            int m = (f+l)/2;
            if(arr[m]==K){
                res = 1;
                break;
            } else if (arr[m]>K) {
                l = m-1;
            } else {
                f = m+1;
            }
        }
        
        return res;
    }
}
